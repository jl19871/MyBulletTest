<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "http://www.asahi-net.or.jp/~cs8k-cyu/bulletml/bulletml.dtd">
<!-- 4-way (but not too powerful -->
<bulletml xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">
	<action label="top">
		<fire>
			<direction type="absolute">160</direction>
			<bullet />
		</fire>
		<fire>
			<direction type="absolute">200</direction>
			<bullet />
		</fire>
		<fire>
			<direction type="absolute">90</direction>
			<bulletRef label="curve" />
		</fire>
		<fire>
			<direction type="absolute">-90</direction>
			<bulletRef label="curve" />
		</fire>
	</action>

	<bullet label="curve">
		<action>
			<changeDirection>
				<direction type="absolute">180</direction>
				<term>20</term>
			</changeDirection>
		</action>
	</bullet>
</bulletml>
