<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "../bulletml.dtd">

<bulletml xmlns="http://www.asahi-net.or.jp/~cs8k-cyu/bulletml">

<action label="top">
  <repeat><times>1</times>
   <action>
    <fire>
     <direction type="aim">0</direction>
     <speed>1+$rank*0.6</speed>
     <bullet/>
    </fire>
    <repeat><times>10</times>
     <action>
      <fire>
       <direction type="sequence">36</direction>
       <speed>1+$rank*0.6</speed>
       <bullet/>
      </fire>
     </action>
    </repeat>
    <wait>30-$rank*20</wait>
   </action>
  </repeat>
 </action>

</bulletml>